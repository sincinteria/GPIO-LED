try:
    while True:                     #work until Ctrl+c
     for i in range(len(array)):
        GPIO.output(2, array[i][0]) #2,3,4,17-number GPIO
        GPIO.output(3, array[i][1])
        GPIO.output(4, array[i][2])
        GPIO.output(17,array[i][3])
        GPIO.output(27,array[i][4])
        GPIO.output(22,array[i][5])
        GPIO.output(10,array[i][6])
        GPIO.output(9 ,array[i][7])
        GPIO.output(11,array[i][8])
        GPIO.output(5, array[i][9])
        GPIO.output(6, array[i][10])
        GPIO.output(13,array[i][11])
        GPIO.output(19,array[i][12])
        GPIO.output(26,array[i][13])
        GPIO.output(21,array[i][14])
        GPIO.output(20,array[i][15])        
        print (array[i])
        sleep(array[i][16])
        
except KeyboardInterrupt:
    GPIO.cleanup()    



 
